    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "passport-openid": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-rc.3",
    "shell-escape": "^0.2.0",
    "socket.io": "^2.0.3",
    "tmp": "^0.0.33",
    "utf-8-validate": "^4.0.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@hakatashi/eslint-config": "^1.4.0",
    "chai": "^4.1.1",
    "eslint": "^5.0.0-alpha.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^5.1.1",
    "prettier-eslint-cli": "^4.7.1",
    "sinon": "^5.0.1-next.1",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "linebreak-style": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0
    }
  }
}
