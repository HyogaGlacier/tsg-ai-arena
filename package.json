    "chalk": "^2.1.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "concat-stream": "^1.6.0",
    "connect-mongo": "^2.0.1",
    "dockerode": "^2.5.0",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.5",
    "express-status-monitor": "^1.0.0",
    "express-validator": "^5.1.2",
    "hexdump-stream": "^0.0.1",
    "lodash": "^4.17.4",
    "lusca": "^1.5.1",
