extends layout

include mixins

block content
  .container.content
    if (turn.submission.user && turn.submission.user._id.equals(user) )|| contest.isEnded()
      p
        pre= turn.submission.code
        
    h4 stdin
    p
      pre= turn.input

    h4 stdout
    p
      pre= turn.stdout

    if (turn.submission.user && turn.submission.user._id.equals(user) )|| contest.isEnded()
      h4 stderr
      p
        pre= turn.stderr
